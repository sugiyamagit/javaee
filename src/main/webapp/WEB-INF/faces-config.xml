<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>todoBean</managed-bean-name>
  <managed-bean-class>perfect.TodoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>listTodo</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>todo</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>employeeBeen</managed-bean-name>
  <managed-bean-class>perfect.EmployeeBeen</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>listEmp</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>meterBean</managed-bean-name>
  <managed-bean-class>perfect.MeterBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>requestCounter</managed-bean-name>
  <managed-bean-class>perfect.RequestCounter</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
  <managed-property>
   <property-name>count</property-name>
   <property-class>java.lang.Integer</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <application>
  <message-bundle>resources.application</message-bundle>
  <locale-config>
   <default-locale>en</default-locale>
  </locale-config>
 </application>
</faces-config>
